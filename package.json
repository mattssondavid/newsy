{
  "name": "newsy",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "scripts": {
    "build": "npm run clean && npm run buildPrepare && npm run tsc && npm run bundle",
    "buildPrepare": "node buildPrepare.config.js",
    "bundle": "node node_modules/rollup/bin/rollup -c rollup.config.js",
    "bundleTest": "node node_modules/rollup/bin/rollup -c rollup.test.bundle.config.js",
    "clean": "npm run cleanDist && npm run cleanBuild && npm run cleanTestBuild",
    "cleanDist": "rm -r dist && mkdir dist",
    "cleanBuild": "rm -r build && mkdir build",
    "cleanTestBuild": "rm -r testbuild && mkdir testbuild",
    "lint": "npm run typeCheck",
    "karma": "npm run bundleTest && npm run karmaStart",
    "karmaStart": "node node_modules/karma/bin/karma start",
    "test": "npm run karma",
    "typeCheck": "node node_modules/typescript/bin/tsc --noEmit",
    "tsc": "node node_modules/typescript/bin/tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattssondavid/newsy.git"
  },
  "author": "David Mattsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattssondavid/newsy/issues"
  },
  "homepage": "https://github.com/mattssondavid/newsy#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@polymer/lit-element": "^0.6.5",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "fs-extra": "^7.0.1",
    "karma": "^3.1.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^5.2.0",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.2.0",
    "sinon": "^7.5.0"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "redux": "^4.0.4",
    "typescript": "^3.6.4"
  }
}
